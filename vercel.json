{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "storybook-static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/storybook/(.*)", "dest": "/storybook/index.html" },
    { "handle": "filesystem" }
  ]
}

{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "storybook-static/**/*",
      "use": "@vercel/static",
      "config": {
        "distDir": "storybook-static"
      }
    }
  ],
  "routes": [
    { "src": "/storybook/(.*)", "dest": "/storybook/index.html" },
    { "handle": "filesystem" }
  ]
}
